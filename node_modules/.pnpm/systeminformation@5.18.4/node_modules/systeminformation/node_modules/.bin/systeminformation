#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/kraken/node_modules/.pnpm/systeminformation@5.18.4/node_modules/systeminformation/lib/node_modules:/home/ubuntu/kraken/node_modules/.pnpm/systeminformation@5.18.4/node_modules/systeminformation/node_modules:/home/ubuntu/kraken/node_modules/.pnpm/systeminformation@5.18.4/node_modules:/home/ubuntu/kraken/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/kraken/node_modules/.pnpm/systeminformation@5.18.4/node_modules/systeminformation/lib/node_modules:/home/ubuntu/kraken/node_modules/.pnpm/systeminformation@5.18.4/node_modules/systeminformation/node_modules:/home/ubuntu/kraken/node_modules/.pnpm/systeminformation@5.18.4/node_modules:/home/ubuntu/kraken/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
