#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.4.9_typescript@5.1.6/node_modules/ts-node/dist/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.4.9_typescript@5.1.6/node_modules/ts-node/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.4.9_typescript@5.1.6/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.4.9_typescript@5.1.6/node_modules/ts-node/dist/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.4.9_typescript@5.1.6/node_modules/ts-node/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.4.9_typescript@5.1.6/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi
