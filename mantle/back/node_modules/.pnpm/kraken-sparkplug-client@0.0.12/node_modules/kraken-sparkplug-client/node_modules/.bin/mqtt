#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/mqtt@4.3.7/node_modules/mqtt/bin/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/mqtt@4.3.7/node_modules/mqtt/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/mqtt@4.3.7/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/mqtt@4.3.7/node_modules/mqtt/bin/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/mqtt@4.3.7/node_modules/mqtt/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/mqtt@4.3.7/node_modules:/home/ubuntu/kraken/mantle/back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mqtt@4.3.7/node_modules/mqtt/bin/mqtt.js" "$@"
else
  exec node  "$basedir/../../../../../mqtt@4.3.7/node_modules/mqtt/bin/mqtt.js" "$@"
fi
